{
  "name": "bridge-integration",
  "version": "1.0.0",
  "description": "Complete Bridge API integration for autonomous AI agent payments",
  "scripts": {
    "setup": "npm install && cd sdk/typescript && npm install && cd ../../backend && npm install && cd ../frontend && npm install",
    "backend": "cd backend && npm run dev",
    "frontend": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "build": "cd backend && npm run build && cd ../frontend && npm run build",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "migrate": "psql $DATABASE_URL < database/schema.sql",
    "seed": "node scripts/seed-data.js"
  },
  "keywords": [
    "bridge",
    "payments",
    "ap2",
    "x402",
    "agents",
    "crypto"
  ],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "concurrently": "^8.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "express": "^4.18.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "next": "^14.0.0",
    "typescript": "^5.0.0",
    "uuid": "^9.0.0"
  }
}
